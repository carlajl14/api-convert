{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ],
    "build": {
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "scripts": {
        "build": "bash install.sh"
      }
    }
  }
  